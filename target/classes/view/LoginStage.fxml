<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<Pane prefHeight="800.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.supc.passenger.controller.LoginStageController">
    <children>
        <Button layoutX="195.0" layoutY="547.0" mnemonicParsing="false" onAction="#eventLogin" text="登录">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Text layoutX="63.0" layoutY="223.0" strokeType="OUTSIDE" strokeWidth="0.0" text="匿名约车系统乘客端">
            <font>
                <Font size="36.0"/>
            </font>
        </Text>
        <Button layoutX="324.0" layoutY="547.0" mnemonicParsing="false" onAction="#eventRegister" text="注册">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <GridPane layoutX="63.0" layoutY="314.0" prefHeight="173.0" prefWidth="324.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="10.0" prefWidth="74.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="250.0" minWidth="10.0" prefWidth="250.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="97.0" minHeight="10.0" prefHeight="92.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="109.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="用户名">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label text="密码" GridPane.rowIndex="1">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="UserName" prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1"/>
                <TextField fx:id="UserPassword" prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
            </children>
        </GridPane>
    </children>
</Pane>
